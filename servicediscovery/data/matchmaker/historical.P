:- import member/2 from basics.
:- import term_to_atom/2 from string.

hist_object_int(ONid,ONid,ultralog) :-
	part(ONid,_,_,_,_).

hist_immediate_memberof_int(ONid,ultralog,UNSPSC,unspsc) :-
	part(ONid,INC,_,_,_),
	inc(INC,_Na,UNSPSC,_NAICS).

hist_immediate_attribute_int(ONid,ultralog,'hasMaterial',ultralog,atom(Mat),'OMS') :-
	part(ONid,_,MatList,_,_),
	member(Mat,MatList).
hist_immediate_attribute_int(ONid,ultralog,'hasProcess',ultralog,atom(Proc),'OMS') :-
	part(ONid,_,_,ProcList,_),
	member(Proc,ProcList).
hist_immediate_attribute_int(ONid,ultralog,'hasPlatform',ultralog,atom(Plat),'OMS') :-
	part(ONid,_,_,_,PlatList),
	member(Plat,PlatList).

%% historical knowledge that provider (actually service) produced the given NSN before
hist_attribute_object_int(ServiceNid,ultralog,'hasNSNProductionHistory',ultralog,NSN,ultralog):-
	nonvar(ServiceNid),
	atom_to_term_check(ServiceNid,category_obj(ServiceRef)),
	javaMessage(ServiceRef,string(ServiceName),getServiceName),
	odm_niin(NSN,ServiceName).
hist_attribute_object_int(ServiceNid,ultralog,'hasNSNProductionHistory',ultralog,NSN,ultralog):-
	var(ServiceNid),
	nonvar(NSN),
	odm_niin(NSN,ServiceName),
	find_service_by_name(ServiceName,ServiceList),
	member(ServiceRef,ServiceList),%% most probably will be list with just one element
	term_to_atom(category_obj(ServiceRef),ServiceNid).

hist_immediate_relationship_int(UNSPSC,unspsc,'hasNAICSCategory',ultralog,NAICS,'ntis-gov:naics:1997') :-
	inc(_,_,UNSPSC,NAICS).
hist_immediate_relationship_int(NAICS,'ntis-gov:naics:1997',coveredBy,ultralog,NAICSDist,'ntis-gov:naics:1997') :-
	naics_distrib(NAICS,_,NAICSDist).


